{
  "name": "nft-checkout-html",
  "version": "0.0.1",
  "description": "HTML version of MintStack package based on Lit",
  "author": "Leon Jin <ultraj0330@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/Luna-Exchange/MintStackPluginHtml.git"
  },
  "type": "module",
  "main": "dist/CheckoutWidget*.js.map",
  "module": "dist/CheckoutWidget*.js.map",
  "types": "dist/CheckoutWidget*.js.map",
  "exports": {
    ".": {
      "import": "./dist/CheckoutWidget*.js.map",
      "require": "./dist/CheckoutWidget*.js.map"
    }
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "scripts": {
    "dev": "parcel index.html --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "shx rm -rf lib/*",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs,iife --outDir ./lib --minify --dts"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.7.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "axios": "^1.1.3",
    "ethers": "^5.7.2",
    "lit": "^2.4.1"
  }
}
